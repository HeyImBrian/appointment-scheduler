<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="788.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.CustomersController">
   <children>
      <Label layoutX="338.0" layoutY="14.0" text="Customers">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <TableView fx:id="customersTable" layoutX="16.0" layoutY="100.0" prefHeight="200.0" prefWidth="756.0">
        <columns>
          <TableColumn id="tableID" fx:id="tableID" prefWidth="51.0" text="ID" />
          <TableColumn id="tableName" fx:id="tableName" prefWidth="188.0" text="Name" />
            <TableColumn id="tableAddress" fx:id="tableAddress" prefWidth="195.0" text="Address" />
            <TableColumn id="tablePostalCode" fx:id="tablePostal" prefWidth="92.0" text="Postal Code" />
            <TableColumn id="tablePhone" fx:id="tablePhone" prefWidth="143.0" text="Phone" />
            <TableColumn id="tablePhone" fx:id="tableDivisionID" prefWidth="86.0" text="Division ID" />
        </columns>
      </TableView>
      <Button fx:id="deleteButton" layoutX="719.0" layoutY="309.0" mnemonicParsing="false" onAction="#delete" prefHeight="25.0" prefWidth="55.0" text="Delete" />
      <Button fx:id="addButton" layoutX="592.0" layoutY="309.0" mnemonicParsing="false" onAction="#switchToAddCustomersForm" prefHeight="25.0" prefWidth="55.0" text="Add" />
      <Button fx:id="updateButton" layoutX="657.0" layoutY="309.0" mnemonicParsing="false" onAction="#switchToUpdateCustomersForm" text="Update" />
      <Button fx:id="cancelButton" layoutX="722.0" layoutY="361.0" mnemonicParsing="false" onAction="#switchToNavigationForm" text="Cancel" />
   </children>
</AnchorPane>
